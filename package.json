{
  "name": "typescript-scripts",
  "version": "1.0.0",
  "description": "A Node.js project for running TypeScript scripts",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "script": "ts-node",
    "generate-readme": "ts-node src/generate_readme.ts",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,md}\""
  },
  "keywords": [
    "typescript",
    "node",
    "scripts"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "ts-node": "^10.9.0",
    "prettier": "^3.1.0"
  },
  "dependencies": {}
}
